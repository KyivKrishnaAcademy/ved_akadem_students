.row.vert-offset-top-1
  .col-xs-12
    - content_for(:js_and_css) do
      = javascript_include_tag 'generated/group-performance-bundle', type: 'text/javascript'
      = stylesheet_link_tag 'range-slider', media: 'all'

    = react_component('GroupPerformanceApp', props: { people: people,
                                                      canManage: policy(ExaminationResult).ui_manage?,
                                                      examinations: examinations,
                                                      localization: attendance_localization,
                                                      examinationResults: examination_results })
